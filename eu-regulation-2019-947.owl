    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#has_ObstacleHeight -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#has_ObstacleHeight">
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#Obstacle"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
