    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#is_BVLOS-Mode -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#is_BVLOS-Mode">
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#UnmannedAircraftSystem"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#is_VLOS-Mode"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#is_Follow-Me-Mode -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#is_Follow-Me-Mode">
