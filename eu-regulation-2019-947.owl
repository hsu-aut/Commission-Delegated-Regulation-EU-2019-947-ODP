    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#has_MaximumOperatingSpeed -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#has_MaximumOperatingSpeed">
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#UnmannedAircraftSystem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Maximum Speed in meters per second</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#has_MaximumTakeOffMass -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#has_MaximumTakeOffMass">
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#UAS_Open"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
