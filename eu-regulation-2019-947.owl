    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#has_Distance_Radial -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#has_Distance_Radial">
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#Distance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#has_Distance_Vertical -->

    <owl:DatatypeProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#has_Distance_Vertical">
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#Distance"/>
