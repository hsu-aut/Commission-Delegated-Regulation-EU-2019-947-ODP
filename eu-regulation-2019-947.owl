    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#has_Distance -->

    <owl:ObjectProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#has_Distance">
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#AssemblyOfPeople"/>
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#UAS_Open"/>
        <rdfs:range rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#Distance"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#has_DroneLicense -->

    <owl:ObjectProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#has_DroneLicense">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#RemotePilot"/>
        <rdfs:range rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#DroneLicense"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#is_carrying -->

    <owl:ObjectProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#is_carrying">
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#UnmannedAircraftSystem"/>
        <rdfs:range rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#DangerousGood"/>
        <rdfs:range rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#Human"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#is_equipped_with"/>
        <rdfs:comment>equivalent to transporting. Equipment is not carried</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#is_dropping -->

    <owl:ObjectProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#is_dropping">
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#UAS_Open"/>
        <rdfs:range rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#Material"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#is_equipped_with -->

    <owl:ObjectProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#is_equipped_with">
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#UnmannedAircraftSystem"/>
        <rdfs:range rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#Equipment"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/hsu-aut/eu-regulation2019-947#is_overflying -->

    <owl:ObjectProperty rdf:about="http://w3id.org/hsu-aut/eu-regulation2019-947#is_overflying">
        <rdfs:domain rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#UAS_Open"/>
        <rdfs:range rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#AssemblyOfPeople"/>
        <rdfs:range rdf:resource="http://w3id.org/hsu-aut/eu-regulation2019-947#Obstacle"/>
    </owl:ObjectProperty>
    


